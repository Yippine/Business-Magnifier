{
  "name": "business-magnifier",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:aitools": "npx ts-node src/lib/database/migration/migrateAITools.ts",
    "docker:mongo": "docker-compose up mongodb -d",
    "docker:tools": "docker-compose --profile tools up -d",
    "docker:full": "docker-compose --profile full-stack up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:connect": "mongosh mongodb://localhost:27017/business-magnifier",
    "db:connect-docker": "mongosh mongodb://admin:password@localhost:27017/business-magnifier?authSource=admin"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "mongoose": "^8.0.3",
